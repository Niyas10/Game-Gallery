<!-- Shopping Cart Section Begin -->
<section class="shopping-cart spad" style="padding-top: 50px; padding-bottom: 50px; margin: 0;">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="continue__btn mb-3 text-center" style="display: flex;
                justify-content: end;  ">
                    <a class="btn btn-primary" href="/shop"  style="    background-color: #ff2f2f;
                    border-color: #ff2f2f;"   >Continue Shopping</a>
                </div>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Product</th>
                                <th></th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <%
                            if (wishlist && wishlist.length) {
                                wishlist.forEach((pdt) => {
                            %>
                            <tr>
                                <td class="product__cart__item">
                                    <div class="row">
                                        <div class="col-md-3 text-center"> <!-- Centered text and image -->
                                            <a href="/shop/productOverview/<%= pdt._id %>">
                                                <img class="img-fluid" src="/assets/images/product/<%= pdt.images[0] %>"
                                                    alt="Product image">
                                            </a>
                                        </div>
                                        <div class="col-md-6 text-center d-flex align-items-center justify-content-center">
    <div style="font-size: 16px;" class="my-auto">
        <h6 style="font-size: 20px;"><%= pdt.name %></h6>
        <h6 style="font-size: 18px;"><%= pdt.brand %></h6>
        <h5 style="font-size: 18px;">&#8377;<%= pdt.price %></h5>
    </div>
</div>

                                    </div>
                                </td>
                                <td class="text-center align-middle"> <!-- Centered button vertically -->
                                    <a class="btn btn-danger"
                                        href="/removeWishlistItem/<%= pdt._id %>?returnPage=wishlist">Remove</a>
                                </td>
                                <td class="text-center align-middle"> <!-- Centered button vertically -->
                                    <a class="btn btn-dark" href="/shop/addToCart/<%= pdt._id %>">Add To Cart</a>
                                </td>
                            </tr>
                            <%
                                })
                            } else {
                            %>
                            <tr>
                                <td colspan="3" class="text-center">
                                    <h5><strong>No wishes yet! Make a new wish</strong></h5>
                                </td>
                            </tr>
                            <%
                                }
                            %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
